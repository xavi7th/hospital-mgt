{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "inertiajs/inertia-laravel": "^0.4.1",
    "intervention/image": "^2.7",
    "laravel/framework": "^8.12",
    "laravel/telescope": "^4.6",
    "laravel/tinker": "^2.5",
    "nwidart/laravel-modules": "^8.2",
    "staudenmeir/belongs-to-through": "^2.11",
    "tightenco/ziggy": "^1.1",
    "wewowweb/laravel-svelte-preset": "^0.1.8"
  },
  "require-dev": {
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "fruitcake/laravel-telescope-toolbar": "^1.3",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "scripts": {
    "compile": [
      "@php artisan storage:link",
      "@php artisan migrate:fresh --force",
      "@php artisan config:clear",
      "@php artisan route:clear",
      "@php artisan cache:clear",
      "@php artisan view:clear",
      "@php artisan event:clear",
      "@php artisan key:generate --ansi",
      "@php artisan module:seed",
      "@php artisan auth:clear-resets",
      "@php artisan route:cache",
      "@php artisan event:cache",
      "@php artisan config:cache",
      "@php artisan optimize",
      "composer dumpautoload"
    ],
    "recompile": [
      "@php artisan storage:link",
      "@php artisan config:clear",
      "@php artisan route:clear",
      "@php artisan cache:clear",
      "@php artisan view:clear",
      "@php artisan event:clear",
      "@php artisan route:cache",
      "@php artisan event:cache",
      "@php artisan config:cache",
      "@php artisan optimize",
      "composer dumpautoload"
    ],
    "reseed": [
      "@php artisan storage:link",
      "@php artisan migrate:fresh",
      "@php artisan config:clear",
      "@php artisan route:clear",
      "@php artisan cache:clear",
      "@php artisan view:clear",
      "@php artisan event:clear",
      "@php artisan module:seed",
      "composer dumpautoload"
    ],
    "tf": "clear && vendor/bin/phpunit --filter",
    "ts": "vendor/bin/phpunit --testsuite",
    "all-tests": "@php artisan test && vendor/bin/phpunit app/Modules",
    "run-artisan-test": "@php artisan test",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
