{
	"compilerOptions": {
		"target": "es2017",
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"paths": {
			"@PublicPages/*": ["main/app/Modules/PublicPages/Resources/js/Pages/*"],
			"@PublicShared/*": ["main/app/Modules/PublicPages/Resources/js/Shared/*"],
			"@PublicAssets/*": ["main/app/Modules/PublicPages/Resources/*"],
			"@userpages/*": ["main/app/Modules/AppUser/Resources/js/Pages/*"],
			"@usershared/*": ["main/app/Modules/AppUser/Resources/js/Shared/*"],
			"@userassets/*": ["main/app/Modules/AppUser/Resources/*"],
			"@superadmin-pages/*": ["main/app/Modules/SuperAdmin/Resources/js/Pages/*"],
			"@superadmin-shared/*": ["main/app/Modules/SuperAdmin/Resources/js/Shared/*"],
			"@superadmin-assets/*": ["main/app/Modules/SuperAdmin/Resources/*"],
		}
	},
	"include": ["main/app", "main/config/", "main/database/", "main/resources/", "main/routes/"]
}
